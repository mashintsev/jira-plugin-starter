<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}" plugins-version="2">
    <plugin-info>
        <description>${project.description}</description>
        <version>${project.version}</version>
        <vendor name="${project.organization.name}" url="${project.organization.url}"/>

        <!-- 16x16px PNG/JPG/GIF       -->
        <param name="vendor-icon">/com/jira/plugins/starter/images/vendor_icon.png</param>
        <!-- 72x72px PNG/JPG/GIF       -->
        <param name="vendor-logo">/com/jira/plugins/starter/images/vendor.png</param>
        <!-- 16x16px PNG/JPG/GIF       -->
        <param name="plugin-icon">/com/jira/plugins/starter/images/logo_icon.png</param>
        <!-- 144 x 144px PNG/JPG       -->
        <param name="plugin-logo">/com/jira/plugins/starter/images/logo.png</param>
        <!-- 1120 x 548px PNG/JPG for high-resolution images or 560 x 274px PNG/JPG for standard images -->
        <param name="plugin-banner">/com/jira/plugins/starter/images/plugin_banner.png</param>

        <param name="configure.url">/secure/StarterPluginConfigureAction.jspa</param>

        <param name="atlassian-data-center-status">compatible</param>
        <param name="atlassian-data-center-compatible">true</param>
    </plugin-info>

    <resource type="i18n" name="i18n" location="com/jira/plugins/starter/i18n"/>

    <rest key="rest" path="/starter" version="1.0"/>

    <ao key="ao-entities">
        <entity>com.jira.plugins.starter.model.Config</entity>
    </ao>

    <velocity-allowlist key="velocity-allowlist-dynamictasklist2">
    </velocity-allowlist>
</atlassian-plugin>
